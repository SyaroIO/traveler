{
    "restartable": "rs",
    "verbose": true,
    "watch": [
        "src",
        "frontend/src",
        "frontend/public",
        "frontend/index.html",
        "frontend/vite.config.ts"
    ],
    "ignore": [
        "log",
        "node_modules",
        "dist",
        "frontent/vite.config.ts.*.mjs"
    ],
    "delay": "1000",
    "exec": "ts-node src/index.ts",
    "ext": "ts ejs yml json"
}