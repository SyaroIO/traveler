{
  "name": "traveler",
  "private": true,
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "fullinstall": "pnpm install && cd frontend && pnpm install",
    "dev": "nodemon --config nodemon.json",
    "test": "node dist",
    "start": "set NODE_ENV=production&&node dist/src",
    "production": "set NODE_ENV=production&&nodemon --config nodemon.json",
    "clean": "rimraf dist",
    "prebuild": "rimraf -g dist/!(frontend)",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build-frontend": "cd frontend && pnpm build",
    "build-all": "pnpm build && pnpm build-frontend ",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint:check": "eslint --no-error-on-unmatched-pattern src",
    "lint:check:fix": "eslint --no-error-on-unmatched-pattern --fix src",
    "lint-frontend": "cd frontend && pnpm lint",
    "lint-frontend:fix": "cd frontend && pnpm lint:fix",
    "lint-frontend:check": "cd frontend && pnpm lint:check",
    "lint-frontend:check:fix": "cd frontend && pnpm lint:check:fix",
    "lint-all": "pnpm lint && pnpm lint-frontend",
    "lint-all:fix": "pnpm lint:fix && pnpm lint-frontend:fix",
    "lint-all:check": "pnpm lint:check && pnpm lint-frontend:check",
    "lint-all:check:fix": "pnpm lint:check:fix && pnpm lint-frontend:check:fix"
  },
  "dependencies": {
    "@koa/router": "^12.0.0",
    "axios": "^1.4.0",
    "jsonwebtoken": "^9.0.1",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-connect": "^2.1.0",
    "koa-proxy": "1.0.0-alpha.3",
    "koa-static": "^5.0.0",
    "log4js": "^6.9.1",
    "mongodb": "^5.7.0",
    "nodemailer": "^6.9.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "@types/koa": "^2.13.7",
    "@types/koa-proxy": "^1.0.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^20.4.5",
    "@types/nodemailer": "^6.4.9",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "eslint": "^8.45.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  }
}